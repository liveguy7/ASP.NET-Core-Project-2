@model PrivacyViewModel

<div class="container">
    <a class="btn btn-primary" style="width:auto" asp-area="" asp-controller="Administration" asp-action="CreateRole">Create New Role</a><br />
    <a class="btn btn-primary" style="width:auto" asp-area="" asp-controller="Administration" asp-action="ListRoles">List Roles</a><br />
</div><br />
<div class="container">
    <form asp-controller="Home" asp-action="Privacy" method="Post">
    <div class="form-group">
            <label asp-for="FirstName" class="col-form-label"></label>
            <input asp-for="FirstName" class="form-control" />
    </div>
    <div class="form-group">
            <label asp-for="Issue" class="col-form-label"></label>
            <input asp-for="Issue" class="form-control" />
    </div>
        <div class="form-group">
            <label asp-for="Manager" class="col-form-label"></label>
            <input asp-for="Manager" class="form-control" />
        </div>
     
    <br />
    <button type="submit" class="btn btn-danger">Submit</button><br />
</form>
</div>


